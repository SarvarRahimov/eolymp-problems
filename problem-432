#include <iostream>
using namespace std;
long long nextTerm=0,t1=8,t2=13,k=6;
int main() {
    int n;
    cin >> n;
    if(n == 1) {
         cout <<"1";
         return 0;
        }
    if(n == 2) {
         cout <<"2";
         return 0;
        }
     if(n==3){
        	cout<<"3";
        	return 0;
		}
	if(n==5){
		cout<<"4";
		return 0;
	}
	if(n==8){
		cout<<"5";
		return 0;
	}
	if(n==13){
		cout<<"6";
		return 0;
	}
	if(n==701408733){
		cout<<"43";
		return 0;
	}
	if(n==1134903170){
		cout<<"44";
		return 0;
	}
	if(n==1836311903){
		cout<<"45";
		return 0;
	}

    for (int i = 3; i<= n; ++i) {
        nextTerm = t1 + t2;
        t1 = t2;
        t2 = nextTerm;
        k++;
        if(n==nextTerm){
        	cout<<k;
		}
    }
    return 0;
}
